/* == Implementation == */

const char* config_version() {
    return TOSTRING(@MULTOVL_VERSION_MAJOR@.@MULTOVL_VERSION_MINOR@) ;
}

const char* config_gitrevision() {
    return TOSTRING(@GITREV_SHA1@) ;
}

const char* config_gitrev() {
    return TOSTRING(@GITREV_SHA1SHORT@) ;
}

const char* config_gitrevdate() {
    return TOSTRING(@GITREV_DATE@) ;
}

